<div class="container">
    <!-- Main component for a primary marketing message or call to action -->
    <div class="row">
        <h1>{{title}}</h1>
        <p>Welcome to {{title}} {{active_about}}</p>
        <p>
          <a href="/todo/new" class="btn btn--blue">New todo</a>
          <hr/>
          <table class="table table-striped table-bordered">
     				<tr>
     					<th>Judul </th>
     					<th>Kategori </th>
     					<th>Isi </th>
     				</tr>
     			 <tbody>
     				 {{#todos}}
     				 <tr>
     						 <td>{{ judul }}</td>
     						 <td>{{ kategori }}</td>
     						 <td>{{ isi }}</td>
     					</tr>
     				 {{/todos}}

     				 </tbody>
     		 </table>

    </div>

</div> <!-- /container -->
